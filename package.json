{
  "name": "http-proxy-to-socks",
  "version": "2.0.0",
  "description": "hpts(http-proxy-to-socks) is a nodejs client to convert socks proxy into http proxy",
  "main": "lib/server.js",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/oyyd/http-proxy-to-socks"
  },
  "scripts": {
    "start": "node ./bin/hpts.js",
    "dev": "vitest",
    "test": "eslint src && vitest run",
    "prepublish": "npm run test"
  },
  "keywords": [
    "socks",
    "http proxy",
    "converting proxy"
  ],
  "bin": {
    "hpts": "./bin/hpts.js"
  },
  "author": "westarne <arne@westarne.de>, oyyd <oyydoibh@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "socks": "^1.1.10",
    "socks-proxy-agent": "^6.0.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "eslint": "^8.56.0",
    "vitest": "^1.2.0"
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "testRegex": "(\\.|/)(test|spec)\\.js$"
  }
}
